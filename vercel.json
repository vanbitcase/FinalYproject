{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/send",
      "methods": ["POST"],
      "dest": "api/index.py"
    },
    {
      "src": "/",
      "dest": "templates/index.html"
    }
  ]
}
